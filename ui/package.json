{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-config-schema": "yarn run json2ts ../schema/config.json > src/config.d.ts",
    "generate-cel-schema": "yarn run json2ts ../schema/cel.json > src/cel.d.ts",
    "generate:schemas": "node scripts/form-generation/index.cjs && yarn run copy:schemas",
    "copy:schemas": "rm -rf public/schema-forms && cp -r schema-forms public/schema-forms",
    "prebuild": "yarn run generate:schemas",
    "predev": "yarn run copy:schemas"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.10",
    "@ant-design/icons": "^5.5.2",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@monaco-editor/react": "^4.7.0",
    "@rjsf/antd": "^5.22.2",
    "@rjsf/core": "^5.22.2",
    "@rjsf/utils": "^5.22.2",
    "@rjsf/validator-ajv8": "^5.22.2",
    "ajv": "^8.18.0",
    "antd": "^5.23.3",
    "chart.js": "^4.4.10",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.0.2",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.562.0",
    "monaco-editor": "^0.55.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.2.1",
    "swr": "^2.2.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@eslint/js": "^9.39.1",
    "@types/js-yaml": "^4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "json-schema-to-typescript": "^15.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  },
  "packageManager": "yarn@4.12.0"
}
